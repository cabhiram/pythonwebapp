{
	"AWSEBDockerrunVersion": 2,
	"containerDefinitions": [{
			"name": "codebase",
			"image": "cabhiram/codebase",
			"hostname": "codebase",
			"essential": false,
      "memory": 500
		},
		{
			"name": "app",
			"image": "cabhiram/app",
			"hostname": "app",
			"essential": true,
			"portMappings": [{
				"hostPort": 80,
				"containerPort": 7000
			}],
      "memory": 500
		}
	]
}
